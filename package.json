{
  "name": "forensic-handler",
  "version": "0.0.0-development",
  "description": "a module that sits independently between the controller and the model, performing request data validation, serialization and integrity checks",
  "main": "lib/Handler",
  "typings": "typings/Handler",
  "scripts": {
    "test": "BABEL_ENV=test jest",
    "watch-test": "npm run test -- --watch",
    "commit": "git-cz",
    "prebuild": "rimraf dist && rimraf lib",
    "build": "BABEL_ENV=build rollup --config",
    "semantic-release": "semantic-release",
    "report-coverage": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "author": "Harrison Ifeanyichukwu <harrisonifeanyichukwu@gmail.com> (https://github.com/harrison-ifeanyichukwu)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/preset-env": "7.4.0",
    "@babel/preset-typescript": "7.3.3",
    "@types/bluebird": "3.5.27",
    "@types/jest": "24.0.11",
    "@types/node": "12.0.7",
    "@types/validator": "10.11.1",
    "@typescript-eslint/eslint-plugin": "1.6.0",
    "@typescript-eslint/parser": "1.6.0",
    "babel-jest": "24.3.1",
    "commitizen": "3.0.7",
    "copy-dir": "0.4.0",
    "coveralls": "3.0.3",
    "cz-conventional-changelog": "2.1.0",
    "dotenv": "8.0.0",
    "jest": "24.3.1",
    "mongoose": "5.3.10",
    "mysql2": "1.6.5",
    "rimraf": "2.6.2",
    "rollup": "0.66.6",
    "rollup-all": "1.6.9",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "9.3.4",
    "rollup-plugin-node-resolve": "3.4.0",
    "rollup-plugin-uglify": "6.0.2",
    "semantic-release": "15.13.16",
    "sequelize": "5.8.7"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "7.4.3",
    "@babel/runtime": "7.4.3",
    "@forensic-js/regex": "1.0.2",
    "@forensic-js/utils": "2.3.0",
    "file-type": "10.11.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/harrison-ifeanyichukwu/forensic-handler.git"
  }
}
